{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "one-var":"off",
        "no-multi-spaces":"off",
        "indent":"off",
        "prefer-destructuring":"off",
        "key-spacing":"off",
        "comma-dangle":"off",
        "strict":"off",
        "no-tabs":"off",
        "no-unused-vars":"off",
        "object-curly-newline":"off",
        "no-multiple-empty-lines":"off"
    }
}
